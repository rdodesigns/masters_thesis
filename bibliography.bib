affiliation = {Ecole Polytechnique F{\'e}d{\'e}rale de Lausanne EPFL, Laboratory of Lanthanide Supramolecular Chemistry LCSL, CH-1015, Lausanne, Switzerland. jean-claude.bunzli@epfl.ch},
doi = {10.1039/b406082m},
