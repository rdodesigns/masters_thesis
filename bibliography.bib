pages = {2495--2508},
month = jun,
affiliation = {Laboratoire Interuniversitaire des Syst{\`e}mes Atmosph{\'e}riques (LISA), CNRS (UMR 7583), Institut Pierre-Simon Laplace, Universit{\'e} Paris Diderot, Universit{\'e} Paris Est Cr{\'e}teil, 94010 Cr{\'e}teil Cedex, France.},
doi = {10.1098/rsta.2011.0272},
